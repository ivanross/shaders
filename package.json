{
  "name": "svelte-kit",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn create:assets && svelte-kit dev",
    "build": "yarn create:assets && svelte-kit build",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
    "create:assets": "node ./scripts/create-assets.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^1.0.0-next.0",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "fs-extra": "^10.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.42.6"
  },
  "type": "module",
  "dependencies": {
    "regl": "^2.1.0"
  }
}
